#include <stdio.h>

// Defining a structure for a Student
struct Student 
{
	int id;
	char name[50];
	float marks;
};

int main() 
{
	// Initializing the structure
	struct Student student1 = {101, "John", 85.5};

	printf("Student ID: %d\n", student1.id);
	printf("Student Name: %s\n", student1.name);
	printf("Student Marks: %.2f\n", student1.marks);

	return 0;
}

